<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="mrp_workcenter_view_form_inherit" model="ir.ui.view">
        <field name="name">mrp.workcenter.form.inherit</field>
        <field name="model">mrp.workcenter</field>
        <field name="inherit_id" ref="mrp.mrp_workcenter_view" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='alternative_workcenter_ids']" position="after">
                <field name="block_same_product" />
                <field name="print_pallet_label" />
            </xpath>
        </field>
    </record>
</odoo>
